generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model Article {
    @@map("articles")
    id        Int      @id @default(autoincrement())
    title     String
    slug      String
    cid       String   @unique
    url       String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    siteKey   String
}
