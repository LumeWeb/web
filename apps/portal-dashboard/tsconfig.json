{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["vite/client"],
    "paths": {
      "~/*": [
        "./apps/portal-dashboard/app/*"
      ],
      "~/components/*": [
        "./apps/portal-dashboard/app/components/*"
      ],
      "~/utils/*": [
        "./apps/portal-dashboard/app/utils/*"
      ]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "extends": "../../tsconfig.base.json"
}
